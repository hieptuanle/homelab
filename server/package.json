{
  "name": "server2",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "fastify-start": "fastify start -l info -a 0.0.0.0 -p 3000 app.js",
    "build-metadata": "bash ./scripts/build-metadata.sh",
    "fastify-dev": "fastify start -w -l info -P -a 0.0.0.0 -p 3000 app.js",
    "start": "run-s build-metadata fastify-start",
    "dev": "run-s build-metadata fastify-dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/http-proxy": "^9.2.1",
    "@fastify/postgres": "^5.2.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/reply-from": "^9.4.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/static": "^6.10.2",
    "@fastify/view": "^7.4.1",
    "close-with-grace": "^1.2.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "eslint": "8.43.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "pg": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "socket.io": "4.7.0",
    "yeelight-node": "^1.1.6"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tap": "^16.1.0"
  }
}
